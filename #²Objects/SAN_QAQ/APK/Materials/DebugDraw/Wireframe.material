Material:
    Layers: [ DebugRenderLayer ]
    Shader: ~res:/Materials/Shaders/DebugDraw/debugdraw
    RenderState:
        depthTest: true
        depthWrite: true
        fillMode: FILLMODE_WIREFRAME
    Passes:
        Forward:
        ForwardWithDepthOnly:
        ReflectionRefraction:
        DepthOnly:
            enabled: false
        ShadowMap:
            enabled: false
